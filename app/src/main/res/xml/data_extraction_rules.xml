<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="database" path="worktime_tracker_db"/>
        <exclude domain="database" path="worktime_tracker_db-wal"/>
        <exclude domain="database" path="worktime_tracker_db-shm"/>
        <include domain="file" path="datastore/settings.preferences_pb"/>
    </cloud-backup>
    <device-transfer>
        <include domain="database" path="worktime_tracker_db"/>
        <exclude domain="database" path="worktime_tracker_db-wal"/>
        <exclude domain="database" path="worktime_tracker_db-shm"/>
        <include domain="file" path="datastore/settings.preferences_pb"/>
    </device-transfer>
</data-extraction-rules>
